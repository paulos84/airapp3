import unittest
import json
from views.charts import get_json
json_data = open('mock_data/my1_data.json')
data = json.load(json_data)
my1_array = data['AirQualityData']['Data']

get_json_array = get_json('MY1', 3)
for d in get_json_array:
    d.update((k, '1') for k, v in d.items() if k == '@Value' and v != 'hours')  # Just change '@Value' as done in test_get_data??
for d in my1_array:
    d.update((k, '1') for k, v in d.items() if k == '@Value' and v != 'hours')

print(get_json_array[0])
print(my1_array[0])

